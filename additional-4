#include<iostream>
#include<cmath>
#include<algorithm>
using namespace std;
int main()
{
    int n=8;
int arr[n]={1,2,0,0,2,1,1,2};
for(int i=0;i<n;i++){
    cout<<arr[i]<<" ";
}
cout<<endl;
int low=0;
int mid=0;
int high=n-1;

while(mid<=high){
if(arr[mid]==2){
    swap(arr[mid],arr[high]);
    high--;
}
if(arr[mid]==0){
    swap(arr[mid],arr[low]);
    mid++;
    low++;
}
if(arr[mid]==1){
    mid++;
}

}
for(int i=0;i<n;i++){
    cout<<arr[i]<<" ";
}


    return 0;
}
