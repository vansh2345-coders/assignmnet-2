#include<iostream>
#include<algorithm>
#include<string>
using namespace std;
int main()
{
    int arr[7]={1,2,3,4,5,6,7};
for(int i=0;i<7;i++){
    cout<<arr[i]<<" ";
}
cout<<endl;
for(int i=0;i<7;i++){
    if(arr[i]==2){
        for(int j=6;j>i;j--){
            arr[j]=arr[j-1];
        }
        arr[i+1]=2;
        i++;
    }
}

    
    for(int i=0;i<7;i++){
        cout<<arr[i]<<" ";
    }






    return 0;
}
