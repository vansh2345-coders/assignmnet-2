#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int main()
{

string str="vansh jain";
cout<<str;
cout<<endl;
sort(str.begin(),str.end());
cout<<str;
cout<<endl;
int arr[str.length()];

  for (int i = 0; i < str.length(); i++) {
        arr[i] = (int)str[i];
    }

for(int i=0;i<str.length();i++){
    for(int j=0;j<str.length()-1-i;j++){
       if(arr[j]>arr[j+1]){
        swap(arr[j],arr[j+1]);
       }
    }
}
for(int i=0;i<str.length();i++){
    cout<<char(arr[i])<<" ";
}
