#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int main()
{
string str="vansh jain";
cout<<str<<endl;
reverse(str.begin(),str.end());
cout<<str;
cout<<endl;

for(int i=0;str[i]!='\0';i++){
    cout<<str[i];
}
cout<<endl;

for(int i=str.length()-1;i>=0;i--){
    cout<<str[i];
}
   return 0;
}
