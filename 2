#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int sum=0;
       int arr[n];
       n=sizeof(arr)/sizeof(arr[0]);
       
       int actualsum;
       for(int i=0;i<n;i++){
        cin>>arr[i];
       }
       for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
       }
       actualsum=(n+1)*(n+2)/2;

        for(int i=0;i<n;i++){
        sum=sum+arr[i];
       }
       int missing;
       missing=actualsum-sum;
       cout<<" missing number is :"<<missing;














return 0;
} 
